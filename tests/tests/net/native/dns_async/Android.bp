cc_defaults {
    name: "dns_async_defaults",

    cflags: [
        "-fstack-protector-all",
        "-g",
        "-Wall",
        "-Wextra",
        "-Werror",
        "-fno-builtin",
    ],
    srcs: [
        "src/NativeDnsAsyncTest.cpp",
    ],
    shared_libs: [
        "liblog",
        "libutils",
        "libandroid",
    ],
}

cc_test {
    name: "CtsNativeNetDnsTestCases",
    defaults: ["dns_async_defaults"],
    multilib: {
        lib32: {
            suffix: "32",
        },
        lib64: {
            suffix: "64",
        },
    },
    test_suites: [
        "cts",
    ],
}